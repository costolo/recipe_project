<div class="row">
	<div class="col-md-2">
		<% @recipes.each do |recipe| %>
			<span><%= recipe.vote_count %> - </span>
			<a href="/recipes/<%= recipe.id %>" class="recipe_link"><%= recipe.name %></a><br>
		<% end %>
	</div>

	<div class="col-md-6">
	</div>

	<div class="comment">
	</div>
</div>

<script src="/js/recipelink.js"></script>





